<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.pva.controllers.PasswordListController">
   <children>
      <AnchorPane>
         <children>
            <Button mnemonicParsing="false" onMouseClicked="#addNewBtnOnClick" prefWidth="145.0" text="Add new" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="5.0" />
            <TextField minWidth="100.0" prefWidth="300.0" AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="5.0" AnchorPane.topAnchor="5.0" />
         </children>
      </AnchorPane>
      <AnchorPane>
         <children>
            <TableView prefHeight="200.0" prefWidth="450.0" AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0">
              <columns>
                <TableColumn prefWidth="150.0" text="Resource" />
                <TableColumn prefWidth="150.0" text="Login" />
                  <TableColumn prefWidth="150.0" text="Password" />
              </columns>
            </TableView>
         </children>
      </AnchorPane>
   </children>
</VBox>
